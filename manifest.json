{
    "name": "UI test",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Cyril Kamal, Amanuel Negussie",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Up and coming Toronto Concerts"
    },
    "background": {
        "service_worker": "background.js" 
    },
    "content_scripts": [
        {
            "js": ["init.js"],
            "css": ["init-styles.css"],
            "matches": ["https://www.google.com/maps/*"]
        }

    ],
    "permissions": ["activeTab", "identity", "identity.email"],
    "host_permissions": ["http://localhost:3001/*"]
}